{
  "name": "todoapp-e2e-tests",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/cypress": "^4.1.0",
    "cypress": "^3.4.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "husky": {
